@model ArtOnWheels.Models.ArtworkDto

<h2>Delete Artwork</h2>
<p>Are you sure you want to delete this artwork?</p>
<p><strong>Title:</strong> @Model.Title</p>
<p><strong>Artist:</strong> @Model.ArtistName</p>
<form asp-action="Delete" method="post">
    <input type="hidden" name="id" value="@Model.ArtworkId" />
    <button type="submit">Delete</button>
</form>
<a href="@Url.Action("List", "ArtworkPage")">Back to List</a>
